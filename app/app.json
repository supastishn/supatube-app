{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": true,
      "package": "com.supastishn.supatube"
    },
    "web": {
      "bundler": "metro",
      "output": "single-page-app",
      "favicon": "./assets/images/favicon.png",
      "router": {
        "type": "hash"
      },
      "publicPath": "/supatube-app/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {},
    "extra": {
      "router": {},
      "eas": {
        "projectId": "39901a7f-19d6-451e-8545-05f6c1e698ca"
      }
    }
  }
}
